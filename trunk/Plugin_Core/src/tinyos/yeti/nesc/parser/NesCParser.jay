enum_specifier									
 : ENUM '{' enumerator_list '}'					
 	{ $$ = $1; } 
 | ENUM '{' enumerator_list ',' '}'					
 	{ $$ = $1; 
  	  SemanticError e = new SemanticError("Trailing commas in enumerations are nonstandard",$4); 
 	  e.severity = IMarker.SEVERITY_WARNING;
 	  warnings.add(e);
 	} 
 | ENUM identifier '{' enumerator_list '}'		
 	{ $$ = $2; } 
 | ENUM identifier '{' enumerator_list ',' '}'		
 	{ $$ = $2; 
  	  SemanticError e = new SemanticError("Trailing commas in enumerations are nonstandard",$5); 
 	  e.severity = IMarker.SEVERITY_WARNING;
 	  warnings.add(e);
 	} 
 | ENUM identifier								
 	{ $$ = $2; } 
 ;

