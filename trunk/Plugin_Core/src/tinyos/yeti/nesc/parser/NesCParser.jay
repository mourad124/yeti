// needed because of unnamed structs/unions
struct_declaration_1 
 :  struct_declarator_list NS_td ';' { $$ = null; } 
 |  ';' NS_td { $$ = null; 
 					SemanticError e = new SemanticError("Unamed struct/union fields are not valid in Sun ANSI/ISO C",$1); 
 					e.severity = IMarker.SEVERITY_WARNING;
 					warnings.add(e);
 				} 
 ;

