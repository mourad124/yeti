<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="tinyos.yeti.debugger.proxyTab" name="tinyos.yeti.debugger.proxyTab" schema="schema/tinyos.yeti.debugger.proxyTab.exsd"/>
   <extension-point id="tinyos.yeti.debugger.launchPrerun" name="tinyos.yeti.debugger.launchPrerun" schema="schema/tinyos.yeti.debugger.launchPrerun.exsd"/>

   <extension
         point="org.eclipse.ui.views">
      <view
            name="Component Variables Browser"
            icon="icons/component.gif"
            category="tinyOS.debug.category.main"
            class="tinyOS.debug.views.NescVariablesViewer"
            id="tinyOS.debug.views.ComponenVariablesView">
      </view>
      <category
            id="tinyOS.debug.category.main"
            name="TinyOS Debug">
      </category>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   
   
   <extension
         point="org.eclipse.ui.editorActions">
      <editorContribution
            id="tinyOS.debug.ui.editor.nesc.RulerExtensions"
            targetID="tinyos.ui.editor.nesc">
         <action
               actionID="RulerDoubleClick"
               class="tinyOS.debug.editorActions.BreakpointRulerDelegate"
               id="tinyOS.debug.ui.editor.nesc.editorActions.BreakpointRulerAction"
               label="BreakpointRulerAction"
               style="push">
         </action>
      </editorContribution>
   </extension>
   
   <extension
         point="org.eclipse.ui.popupMenus">
         <viewerContribution
            targetID="#NESCRulerContext"
            id="tinyOS.debug.ui.editor.nesc.BreakpointRulerPopupMenu">
         <action
               label="Toggle Breakpoint"
               class="tinyOS.debug.editorActions.BreakpointRulerDelegate"
               menubarPath="debug"
               id="tinyOS.debug.ui.editor.nesc.editorActions.BreakpointRulerPopupMenu">
         </action>
      </viewerContribution>
   </extension>
   <extension
         point="tinyos.yeti.core.EditorAnnotationModelProvider">
      <factory
            class="tinyOS.debug.views.annotation.DebugMarkerAnnotationModelFactory"
            contentTypeId="TinyOS.nesCSource">
      </factory>
   </extension>

   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="tinyOS.debug.launch.configuration.LaunchConfigurationTabGroup"
            id="tinyOS.debug.launch.tabGroup"
            type="tinyOS.debug.launch.configurationType">
      </launchConfigurationTabGroup>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="tinyOS.debug.launch.LaunchConfigurationDelegate"
            id="tinyOS.debug.launch.configurationType"
            modes="debug"
            name="TinyOS Debugger"
            public="true"
            sourceLocatorId="org.eclipse.cdt.debug.core.sourceLocator"
            sourcePathComputerId="org.eclipse.cdt.debug.core.sourcePathComputer">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="tinyOS.debug.launch.configurationType"
            icon="icons/nescfile.png"
            id="tinyos.launch.build.icon">
      </launchConfigurationTypeImage>
   </extension>
   <extension
         point="tinyos.yeti.core.EditorInputConverter">
      <converter
            class="tinyOS.debug.ep.ExternalEditorInputConverter">
      </converter>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.debug.ui.DebugPerspective">
         <viewShortcut
               id="tinyOS.debug.views.ComponenVariablesView">
         </viewShortcut>
      </perspectiveExtension>
   </extension>
   <extension
         point="tinyos.yeti.debugger.proxyTab">
      <tab
            class="tinyOS.debug.launch.configuration.AvariceGdbProxyTab"
            order="1.5"></tab>
      <tab
            class="tinyOS.debug.launch.configuration.UserDefinedGdbProxyTab"
            order="2.5"></tab>
      <tab
            class="tinyOS.debug.launch.configuration.NoGdbProxyTab"
            order="3.5"></tab>
      <tab
            class="tinyOS.debug.launch.configuration.MSP430ProxyTab"
            order="1.6">
      </tab>
   </extension>
   <extension
         point="tinyos.yeti.debugger.launchPrerun">
      <prerun
            class="tinyOS.debug.launch.configuration.prerun.DoNothingPrerun"
            id="do-nothing"
            order="1.0"
            tab-class="tinyOS.debug.launch.configuration.prerun.DoNothingPrerun">
      </prerun>
      <prerun
            class="tinyOS.debug.launch.configuration.prerun.DefaultPrerun"
            id="build-default"
            order="2.0"
            tab-class="tinyOS.debug.launch.configuration.prerun.DefaultPrerun">
      </prerun>
      <prerun
            class="tinyOS.debug.launch.configuration.prerun.MakeOptionPrerun"
            id="make-option"
            order="3.0"
            tab-class="tinyOS.debug.launch.configuration.prerun.MakeOptionPrerun">
      </prerun>
      <prerun
            class="tinyOS.debug.launch.configuration.prerun.LaunchConfigurationPrerun"
            id="launch-configuration"
            order="2.5"
            tab-class="tinyOS.debug.launch.configuration.prerun.LaunchConfigurationPrerun">
      </prerun>
   </extension>
   
</plugin>
